<system name="MD-11: FCS">
	
	<channel name="Libraries">
		
		<switch name="hydraulics/aileron-out-enable">
			<default value="0"/>
			<test logic="OR" value="1">
				fcs/slat-pos-deg ge 2
				position/wow eq 1
			</test>
			<output>fcc/aileron-out-enable</output>
		</switch>
		
		<!-- Deflected Aileron System -->
		<switch name="hydraulics/deflected-aileron/flaps">
			<default value="0"/>
			<test logic="OR" value="1">
				/controls/flight/flaps-input eq 2
				/controls/flight/flaps-input eq 3
			</test>
		</switch>
		
		<switch name="hydraulics/deflected-aileron/cmd"> <!-- Flip flop controller -->
			<default value="hydraulics/deflected-aileron/cmd"/>
			<test value="0">
				hydraulics/deflected-aileron/flaps ne 1
			</test>
			<test logic="AND" value="1">
				hydraulics/deflected-aileron/flaps eq 1
				/gear/gear[0]/wow eq 1
			</test>
		</switch>
		
		<pure_gain name="hydraulics/deflected-aileron/active">
			<input>hydraulics/deflected-aileron/cmd</input>
			<gain>/systems/acconfig/options/deflected-aileron-equipped</gain>
		</pure_gain>
		
		<switch name="hydraulics/deflected-aileron/in-l-rate">
			<default value="0.575"/> <!-- About 1 second -->
			<test value="100">
				/systems/acconfig/autoconfig-running eq 1
			</test>
			<test logic="AND" value="0.55"> <!-- Slightly slower when deflectioned electronically instead of ADA -->
				/it-autoflight/internal/active-fms eq 1
				<test logic="OR">
					fcc/rcws/active-no-wow eq 1
					fcc/afs/enabled eq 1
				</test>
			</test>
		</switch>
		
		<switch name="hydraulics/deflected-aileron/in-r-rate">
			<default value="0.575"/> <!-- About 1 second -->
			<test value="100">
				/systems/acconfig/autoconfig-running eq 1
			</test>
			<test logic="AND" value="0.55"> <!-- Slightly slower when deflectioned electronically instead of ADA -->
				/it-autoflight/internal/active-fms eq 2
				<test logic="OR">
					fcc/rcws/active-no-wow eq 1
					fcc/afs/enabled eq 1
				</test>
			</test>
		</switch>
		
		<pure_gain name="hydraulics/deflected-aileron/in-cmd"> <!-- 11.5 degrees -->
			<input>hydraulics/deflected-aileron/active</input>
			<gain>0.575</gain> <!-- 11.5 degrees -->
		</pure_gain>
		
		<actuator name="hydraulics/deflected-aileron/in-l">
			<input>hydraulics/deflected-aileron/in-cmd</input> 
			<rate_limit>hydraulics/deflected-aileron/in-l-rate</rate_limit>
		</actuator>
		
		<actuator name="hydraulics/deflected-aileron/in-r">
			<input>hydraulics/deflected-aileron/in-cmd</input>
			<rate_limit>hydraulics/deflected-aileron/in-r-rate</rate_limit>
		</actuator>
		
		<switch name="hydraulics/deflected-aileron/out-rate">
			<default value="0.55"/> <!-- About 1 second -->
			<test value="100">
				/systems/acconfig/autoconfig-running eq 1
			</test>
		</switch>
		
		<switch name="hydraulics/deflected-aileron/out-cmd">
			<default value="0"/>
			<test logic="AND" value="0.75"> <!-- 15 degrees drooped -->
				/systems/acconfig/options/deflected-aileron-equipped eq 1
				hydraulics/deflected-aileron/active eq 1
			</test>
			<test value="0.2"> <!-- 4 degrees normal -->
				/systems/acconfig/options/deflected-aileron-equipped eq 1
			</test>
		</switch>
		
		<actuator name="hydraulics/deflected-aileron/out">
			<input>hydraulics/deflected-aileron/out-cmd</input>
			<rate_limit>hydraulics/deflected-aileron/out-rate</rate_limit>
		</actuator>
	
	</channel>
	
	<channel name="Aileron General">
		
		<pure_gain name="hydraulics/aileron-small">
			<input>/controls/flight/aileron</input>
			<gain>0.25</gain>
		</pure_gain>
		
		<pure_gain name="hydraulics/aileron/trim-overall">
			<input>/controls/flight/aileron-trim</input>
			<gain>0.25</gain>
		</pure_gain>
		
		<switch name="hydraulics/aileron-yoke-switch">
			<default value="/controls/flight/aileron"/>
			<test logic="AND" value="hydraulics/aileron-small"> <!-- While in detent and RCWS active, more force needed to break out -->
				fcc/rcws/active eq 1
				fcc/rcws/in-detent eq 1
			</test>
			<test value="fcc/afs/roll-cmd">
				fcc/afs/enabled eq 1
			</test>
		</switch>
		
		<!-- RCWS + Aileron Droop Wheel Backdrive -->
		<!-- Caused by FCC being slower than ADA on the RCWS controlled aileron -->
		<switch name="hydraulics/aileron/backdrive/direction-1">
			<default value="-1"/>
			<test value="1">
				/it-autoflight/internal/active-fms eq 2
			</test>
		</switch>
		
		<switch name="hydraulics/aileron/backdrive/direction-2">
			<default value="-1"/>
			<test value="1">
				hydraulics/deflected-aileron/active eq 1
			</test>
		</switch>
		
		<summer name="hydraulics/aileron/backdrive/error">
			<input>hydraulics/deflected-aileron/in-l</input>
			<input>-hydraulics/deflected-aileron/in-r</input>
		</summer>
		
		<switch name="hydraulics/aileron/backdrive/active">
			<default value="0"/>
			<test logic="OR" value="1">
				hydraulics/aileron/backdrive/error le -0.01
				hydraulics/aileron/backdrive/error ge 0.01
			</test>
		</switch>
		
		<fcs_function name="hydraulics/aileron/backdrive/cmd">
			<function>
				<product>
					<property>/systems/acconfig/options/deflected-aileron-equipped</property>
					<property>/systems/acconfig/options/rcws-equipped</property>
					<property>hydraulics/aileron/backdrive/active</property>
					<property>hydraulics/aileron/backdrive/direction-1</property>
					<property>hydraulics/aileron/backdrive/direction-2</property>
					<value>0.13</value> <!-- 12 degrees -->
				</product>
			</function>
		</fcs_function>
		
		<actuator name="hydraulics/aileron/backdrive/output">
			<input>hydraulics/aileron/backdrive/cmd</input>
			<rate_limit>0.75</rate_limit>
		</actuator>
		<!-- End RCWS + Aileron Droop Wheel Backdrive -->
		
		<summer name="hydraulics/aileron-yoke-sum">
			<input>hydraulics/aileron-yoke-switch</input>
			<input>hydraulics/aileron/trim-overall</input>
			<input>hydraulics/aileron/backdrive/output</input>
			<clipto>
				<min>-1.0</min>
				<max>1.0</max>
			</clipto>
		</summer>
		
		<summer name="hydraulics/aileron/output">
			<input>fcc/roll-output</input>
			<input>hydraulics/aileron/trim-overall</input>
			<clipto>
				<min>-1.0</min>
				<max>1.0</max>
			</clipto>
		</summer>
		
		<!-- 
		Undoes summing aileron with trim, but keeps the clamp so it works properly
		Without it, we could move the surfaces more than the yoke, must be done this way
		Because the deflection of ailerons and spoiler to trim is not linear like aileron
		So we can't just sum and clip aileron and send it to the surfaces - trust me, I tried
		-->
		<summer name="hydraulics/aileron/output-surfaces">
			<input>hydraulics/aileron/output</input>
			<input>-hydraulics/aileron/trim-overall</input>
			<clipto>
				<min>-1.0</min>
				<max>1.0</max>
			</clipto>
		</summer>
		
		<fcs_function name="hydraulics/aileron/trim-left"> <!-- 7/20 -->
			<function>
				<table>
					<independentVar lookup="row">/controls/flight/aileron-trim</independentVar>
					<tableData>
						-1.0 -0.35
						 0.0  0.00
						 1.0  0.35
					</tableData>
				</table>
			</function>
		</fcs_function>
		
		<fcs_function name="hydraulics/aileron/trim-right"> <!-- 7/20 -->
			<function>
				<table>
					<independentVar lookup="row">/controls/flight/aileron-trim</independentVar>
					<tableData>
						-1.0  0.35
						 0.0  0.00
						 1.0 -0.35
					</tableData>
				</table>
			</function>
		</fcs_function>
		
		<fcs_function name="hydraulics/aileron-droop">
			<function>
				<table>
					<independentVar lookup="row">aero/qbar-psf</independentVar>
					<tableData>
						 7  1
						31  0
					</tableData>
				</table>
			</function>
		</fcs_function>
		
		<fcs_function name="hydraulics/aileron-in-droop-rate">
			<function>
				<table>
					<independentVar lookup="row">aero/qbar-psf</independentVar>
					<tableData>
						 7  0.333333
						31  2
					</tableData>
				</table>
			</function>
		</fcs_function>
		
		<fcs_function name="hydraulics/aileron-out-droop-rate">
			<function>
				<table>
					<independentVar lookup="row">aero/qbar-psf</independentVar>
					<tableData>
						 7  0.066667
						31  2
					</tableData>
				</table>
			</function>
		</fcs_function>
	
	</channel>
	
	<channel name="Aileron L Outer">
		
		<fcs_function name="hydraulics/aileron-out-l/function">
			<function>
				<sum>
					<product>
						<sum>
							<table>
								<independentVar lookup="row">hydraulics/aileron/output-surfaces</independentVar>
								<tableData>
									-0.95 -1.0
									 0.00  0.0
									 0.95  1.0
								</tableData>
							</table>
							<property>hydraulics/aileron/trim-left</property> <!-- Slew neutral point -->
						</sum>
						<property>hydraulics/aileron-out-enable</property>
					</product>
					<property>hydraulics/deflected-aileron/out</property>
				</sum>
			</function>
		</fcs_function>
		
		<switch name="hydraulics/aileron-out-l/switch">
			<default value="hydraulics/aileron-droop"/>
			<test logic="OR" value="hydraulics/aileron-out-l/function">
				/systems/hydraulics/sys-2-psi ge 2200
				/systems/hydraulics/sys-3-psi ge 2200
			</test>
			<clipto>
				<min>-1.0</min>
				<max>1.0</max>
			</clipto>
		</switch>
		
		<pure_gain name="hydraulics/aileron-out-l/cmd-deg">
			<input>hydraulics/aileron-out-l/switch</input>
			<gain>20</gain>
		</pure_gain>
		
		<switch name="hydraulics/aileron-out-l/rate">
			<default value="hydraulics/aileron-out-droop-rate"/>
			<test logic="OR" value="80">
				/systems/hydraulics/sys-2-psi ge 2200
				/systems/hydraulics/sys-3-psi ge 2200
			</test>
		</switch>
		
		<actuator name="hydraulics/aileron-out-l/final-actuator">
			<input>hydraulics/aileron-out-l/cmd-deg</input>
			<rate_limit>hydraulics/aileron-out-l/rate</rate_limit>
			<lag>26.5</lag>
			<output>hydraulics/aileron-out-l/final-deg</output>
		</actuator>
	
	</channel>
	
