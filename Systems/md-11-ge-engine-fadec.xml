		<switch name="fadec/limit/active">
			<default value="100"/>
			<test value="fadec/limit/takeoff">
				fadec/limit/active-mode-int eq 0
			</test>
			<test value="fadec/limit/go-around">
				fadec/limit/active-mode-int eq 1
			</test>
			<test value="fadec/limit/mct">
				fadec/limit/active-mode-int eq 2
			</test>
			<test value="fadec/limit/climb">
				fadec/limit/active-mode-int eq 3
			</test>
			<test value="fadec/limit/cruise">
				fadec/limit/active-mode-int eq 4
			</test>
		</switch>
		
		<fcs_function name="fadec/limit/max">
			<function>
				<ifthen> <!-- When in takeoff, enforce takeoff, else take highest -->
					<eq>
						<property>fadec/limit/active-mode-int</property>
						<value>0</value>
					</eq>
					<property>fadec/limit/takeoff</property>
					<max> <!-- MCT is always higher than CLB or CRZ, so we don't need to check them -->
						<property>fadec/limit/go-around</property>
						<property>fadec/limit/mct</property>
					</max>
				</ifthen>
			</function>
		</fcs_function>
	
