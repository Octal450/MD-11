				<independentVar lookup="row">fcc/aileron-out-enable</independentVar>
				<tableData>
					0  1.00
					1  0.72
				</tableData>
			</table>
		</scheduled_gain>
		
		<scheduled_gain name="fcc/rcws/i-gain-scheduled"> <!-- Schedule gain to compensate for inactive ailerons -->
			<input>fcc/rcws/i-gain</input>
			<table>
				<independentVar lookup="row">fcc/aileron-out-enable</independentVar>
				<tableData>
					0  1.00
					1  0.72
				</tableData>
			</table>
		</scheduled_gain>
		
		<scheduled_gain name="fcc/rcws/d-gain-scheduled"> <!-- Schedule gain to compensate for inactive ailerons -->
			<input>fcc/rcws/d-gain</input>
			<table>
				<independentVar lookup="row">fcc/aileron-out-enable</independentVar>
