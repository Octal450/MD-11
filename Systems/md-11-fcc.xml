<system name="MD-11: FCC">
	
	<property value="0">fcc/aileron-out-enable</property>
	<property value="0">fcc/elevator-feel/manual</property>
	<property value="0">fcc/elevator-feel/speed</property>
	<property value="0">fcc/flap/cmd-deg</property>
	<property value="0">fcc/lsas/autotrim-inhibit</property>
	<property value="0">fcc/lsas/pah/output</property>
	<property value="0">fcc/lsas/spd-prot/output</property>
	<property value="0">fcc/rcws/pid</property>
	<property value="0">fcc/slat/cmd-deg</property>
	<property value="0">fcc/yaw/tc-pd</property>
	<property value="0">fcs/spoiler-left-feedback</property>
	<property value="0">fcs/spoiler-right-feedback</property>
	<property value="0">spoilers/auto-speedbrake-feedback</property>
	<property value="0">/gear/abs/brake-cmd</property>
	<property value="0">/gear/abs/brake-cmd-v</property>
	<property value="0">/gear/abs/disarm</property>
	<property value="0">/gear/abs/spoilers-cmd-deg</property>
	
	<!-- Properties for tuning -->
	<property value="0">fcc/rcws/p-gain-tune</property>
	<property value="0">fcc/rcws/i-gain-tune</property>
	<property value="0">fcc/rcws/d-gain-tune</property>
	<property value="0">fcc/lsas/pah/p-gain-tune</property>
	<property value="0">fcc/lsas/pah/i-gain-tune</property>
	<property value="0">fcc/lsas/pah/d-gain-tune</property>
	<property value="0">fcc/lsas/prd/p-gain-tune</property>
	<property value="0">fcc/yaw/yd-p-gain-tune</property>
	<property value="0">fcc/yaw/tc-p-gain-tune</property>
	<property value="0">fcc/yaw/tc-d-gain-tune</property>
	
	<!-- AFS stuff -->
	<property value="0">fcc/afs/roll-cmd</property>
	<property value="0">fcc/afs/pitch-cmd</property>
	<property value="0">fcc/afs/yaw-cmd</property>
	
	<channel name="Libraries">
		
		<switch name="fcc/fcc1-power">
			<default value="0"/>
			<test logic="OR" value="1">
				/systems/electrical/bus/dc-1 ge 24
				/systems/electrical/bus/l-emer-dc ge 24
			</test>
		</switch>
		
		<switch name="fcc/fcc2-power">
			<default value="0"/>
			<test logic="OR" value="1">
				/systems/electrical/bus/dc-3 ge 24
				/systems/electrical/bus/r-emer-dc ge 24
			</test>
		</switch>
		
		<switch name="fcc/afs/enabled">
			<default value="0"/>
			<test logic="OR" value="1">
				/it-autoflight/output/ap1 eq 1
				/it-autoflight/output/ap2 eq 1
			</test>
		</switch>
		
		<fcs_function name="fcc/stall-alpha-deg">
			<function>
				<table>
					<independentVar lookup="row">atmosphere/density-altitude</independentVar>
					<tableData>
						    0  15.5
						43000   8.3
					</tableData>
				</table>
			</function>
		</fcs_function>
		
		<pure_gain name="fcc/stall-warn-alpha-deg">
			<input>fcc/stall-alpha-deg</input>
			<gain>0.8</gain> <!-- 80% -->
		</pure_gain>
		
		<pure_gain name="fcc/no-stall-warn-alpha-deg">
			<input>fcc/stall-alpha-deg</input>
			<gain>0.75</gain> <!-- 75% -->
		</pure_gain>
		
		<switch name="fcc/pitch-trim-speed">
			<default value="0.325"/>
			<test logic="OR" value="0.175">
				/instrumentation/airspeed-indicator/indicated-speed-kt ge 249.5
				/instrumentation/altimeter/indicated-altitude-ft ge 32990
			</test>
		</switch>
		
		<fcs_function name="fcc/rcws/p-rad_sec">
			<function>
				<toradians>
					<property>/orientation/roll-rate-degps</property>
				</toradians>
			</function>
		</fcs_function>
		
		<fcs_function name="fcc/lsas/thetadot-rad_sec">
			<function>
				<toradians>
					<property>/orientation/pitch-rate-degps</property>
				</toradians>
			</function>
		</fcs_function>
	
	</channel>
	
	<channel name="LSAS Logic">
		
		<switch name="fcc/lsas/left-out-active">
			<default value="0"/>
			<test logic="OR" value="0"> <!-- AP disc button kills LSAS -->
				/controls/cockpit/ap-disc-1 eq 1
				/controls/cockpit/ap-disc-2 eq 1
			</test>
			<test logic="AND" value="1">
				/controls/fcc/switches/lsas-left-out eq 1
				<test logic="OR">
					/systems/hydraulics/sys-1-psi ge 2200
					/systems/hydraulics/sys-2-psi ge 2200
				</test>
				fcc/fcc2-power eq 1
				/systems/failures/fcc/lsas-left-out ne 1
				/systems/iru-common/any-aligned-out eq 1
			</test>
		</switch>
		
		<switch name="fcc/lsas/left-in-active">
			<default value="0"/>
			<test logic="OR" value="0"> <!-- AP disc button kills LSAS -->
				/controls/cockpit/ap-disc-1 eq 1
				/controls/cockpit/ap-disc-2 eq 1
			</test>
			<test logic="AND" value="1">
				/controls/fcc/switches/lsas-left-in eq 1
				<test logic="OR">
					/systems/hydraulics/sys-2-psi ge 2200
					/systems/hydraulics/sys-3-psi ge 2200
				</test>
				fcc/fcc1-power eq 1
				/systems/failures/fcc/lsas-left-in ne 1
				/systems/iru-common/any-aligned-out eq 1
			</test>
		</switch>
		
		<switch name="fcc/lsas/right-in-active">
			<default value="0"/>
			<test logic="OR" value="0"> <!-- AP disc button kills LSAS -->
				/controls/cockpit/ap-disc-1 eq 1
				/controls/cockpit/ap-disc-2 eq 1
			</test>
			<test logic="AND" value="1">
				/controls/fcc/switches/lsas-right-in eq 1
				<test logic="OR">
					/systems/hydraulics/sys-1-psi ge 2200
					/systems/hydraulics/sys-3-psi ge 2200
				</test>
				fcc/fcc2-power eq 1
				/systems/failures/fcc/lsas-right-in ne 1
				/systems/iru-common/any-aligned-out eq 1
			</test>
		</switch>
		
		<switch name="fcc/lsas/right-out-active">
			<default value="0"/>
			<test logic="OR" value="0"> <!-- AP disc button kills LSAS -->
				/controls/cockpit/ap-disc-1 eq 1
				/controls/cockpit/ap-disc-2 eq 1
			</test>
			<test logic="AND" value="1">
				/controls/fcc/switches/lsas-right-out eq 1
				<test logic="OR">
					/systems/hydraulics/sys-1-psi ge 2200
					/systems/hydraulics/sys-2-psi ge 2200
				</test>
				fcc/fcc1-power eq 1
				/systems/failures/fcc/lsas-right-out ne 1
				/systems/iru-common/any-aligned-out eq 1
			</test>
		</switch>
		
		<summer name="fcc/lsas/channels-active">
			<input>fcc/lsas/left-out-active</input>
			<input>fcc/lsas/left-in-active</input>
			<input>fcc/lsas/right-in-active</input>
			<input>fcc/lsas/right-out-active</input>
		</summer>
		
		<fcs_function name="fcc/lsas/spd-prot/max">
			<function>
				<sum>
					<property>/fms/speeds/vmo-mmo</property>
					<ifthen>
						<or>
							<and>
								<eq>
									<property>/it-autoflight/internal/throttle-saturated</property>
									<value>1</value>
								</eq>
								<eq>
									<property>/it-autoflight/output/spd-prot</property>
									<value>0</value>
								</eq>
							</and>
							<nq>
								<property>/it-autoflight/input/athr-avail</property>
								<value>1</value>
							</nq>
						</or>
						<value>5</value>
						<value>10</value>
					</ifthen>
				</sum>
			</function>
		</fcs_function>
		
		<switch name="fcc/lsas/spd-prot/active">
			<default value="fcc/lsas/spd-prot/active"/>
			<test logic="OR" value="0">
				fcc/lsas/channels-active eq 0
				fcc/afs/enabled eq 1
				position/wow eq 1
				attitude/pitch-rad gt 0.558505
				attitude/pitch-rad lt -0.20944
				attitude/roll-rad gt 0.558505
				attitude/roll-rad lt -0.558505
				/instrumentation/airspeed-indicator/indicated-speed-kt le /fms/speeds/vmo-mmo
			</test>
			<test value="1">
				/instrumentation/airspeed-indicator/indicated-speed-kt ge fcc/lsas/spd-prot/max
			</test>
		</switch>
		
		<switch name="fcc/lsas/alpha-prot/active">
			<default value="fcc/lsas/alpha-prot/active"/>
			<test logic="OR" value="0">
				fcc/lsas/channels-active eq 0
				fcc/afs/enabled eq 1
				position/wow eq 1
				attitude/pitch-rad gt 0.558505
				attitude/pitch-rad lt -0.20944
				attitude/roll-rad gt 0.558505
				attitude/roll-rad lt -0.558505
				aero/alpha-deg-damped le fcc/no-stall-warn-alpha-deg
			</test>
			<test value="1">
				aero/alpha-deg-damped ge fcc/stall-warn-alpha-deg
			</test>
		</switch>
		
		<switch name="fcc/lsas/in-detent">
			<default value="fcc/lsas/in-detent"/>
			<test logic="AND" value="1">
				/controls/flight/elevator lt 0.02
				/controls/flight/elevator gt -0.02
			</test>
			<test logic="OR" value="0">
				/controls/flight/elevator gt 0.045
				/controls/flight/elevator lt -0.045
			</test>
		</switch>
		
		<switch name="fcc/lsas/pah/active">
			<default value="0"/>
			<test logic="AND" value="1">
				fcc/lsas/channels-active gt 0
				fcc/afs/enabled eq 0
				fcc/lsas/spd-prot/active ne 1
				fcc/lsas/alpha-prot/active ne 1
				attitude/pitch-rad le 0.558505
				attitude/pitch-rad ge -0.20944
				attitude/roll-rad le 0.558505
				attitude/roll-rad ge -0.558505
				fcc/lsas/in-detent eq 1
				position/wow eq 0
				/position/gear-agl-ft ge 100
			</test>
		</switch>
		
		<switch name="fcc/lsas/prd/active">
			<default value="0"/>
			<test value="1">
				fcc/lsas/spd-prot/active eq 1
			</test>
			<test logic="AND" value="1">
				fcc/lsas/channels-active gt 0
				fcc/afs/enabled eq 0
				<test logic="OR">
					fcc/lsas/in-detent eq 0
					attitude/roll-rad gt 0.558505
					attitude/roll-rad lt -0.558505
				</test>
				position/wow eq 0
				/position/gear-agl-ft ge 100
			</test>
		</switch>
		
		<switch name="fcc/lsas/pap/active">
			<default value="0"/>
			<test logic="AND" value="1">
				fcc/lsas/channels-active gt 0
				fcc/afs/enabled eq 0
				position/wow eq 1
			</test>
			<test logic="AND" value="1">
				fcc/lsas/channels-active gt 0
				fcc/afs/enabled eq 0
				/position/gear-agl-ft le 100
			</test>
		</switch>
		
		<switch name="fcc/lsas/pnl/active">
			<default value="0"/>
			<test logic="AND" value="1">
				fcc/lsas/channels-active gt 0
				fcc/afs/enabled eq 0
				gear/unit[1]/WOW eq 1
				gear/unit[2]/WOW eq 1
				spoilers/auto-speedbrake-feedback gt 0
			</test>
		</switch>
	
	</channel>
	
	<channel name="Yaw Damper Logic">
		
		<switch name="fcc/yaw/avail-upper-a">
			<default value="0"/>
			<test logic="AND" value="1">
				/controls/fcc/switches/yd-upper-a eq 1
				<test logic="OR">
					/systems/hydraulics/sys-1-psi ge 2200
					/systems/hydraulics/nrmp/sys-2-to-sys-1 ge 2200
				</test>
				/systems/electrical/bus/dc-3 ge 24
				/systems/failures/fcc/yd-upper-a ne 1
				/systems/iru-common/any-aligned-out eq 1
			</test>
		</switch>
		
		<switch name="fcc/yaw/avail-upper-b">
			<default value="0"/>
			<test logic="AND" value="1">
				/controls/fcc/switches/yd-upper-b eq 1
				<test logic="OR">
					/systems/hydraulics/sys-1-psi ge 2200
					/systems/hydraulics/nrmp/sys-2-to-sys-1 ge 2200
				</test>
				/systems/electrical/bus/dc-3 ge 24
				/systems/failures/fcc/yd-upper-b ne 1
				/systems/iru-common/any-aligned-out eq 1
			</test>
		</switch>
		
		<switch name="fcc/yaw/avail-lower-a">
			<default value="0"/>
			<test logic="AND" value="1">
				/controls/fcc/switches/yd-lower-a eq 1
				<test logic="OR">
					/systems/hydraulics/sys-2-psi ge 2200
					/systems/hydraulics/nrmp/sys-3-to-sys-2 ge 2200
				</test>
				/systems/electrical/bus/dc-1 ge 24
				/systems/failures/fcc/yd-lower-a ne 1
				/systems/iru-common/any-aligned-out eq 1
			</test>
		</switch>
		
		<switch name="fcc/yaw/avail-lower-b">
			<default value="0"/>
			<test logic="AND" value="1">
				/controls/fcc/switches/yd-lower-b eq 1
				<test logic="OR">
					/systems/hydraulics/sys-2-psi ge 2200
					/systems/hydraulics/nrmp/sys-3-to-sys-2 ge 2200
				</test>
				/systems/electrical/bus/dc-1 ge 24
				/systems/failures/fcc/yd-lower-b ne 1
				/systems/iru-common/any-aligned-out eq 1
			</test>
		</switch>
		
		<switch name="fcc/yaw/active">
			<default value="0"/>
			<test logic="OR" value="0">
				position/wow eq 1
				/position/gear-agl-ft lt 50
			</test>
			<test logic="OR" value="1">
				fcc/yaw/avail-upper-a eq 1
				fcc/yaw/avail-upper-b eq 1
				fcc/yaw/avail-lower-a eq 1
				fcc/yaw/avail-lower-b eq 1
			</test>
		</switch>
		
		<switch name="fcc/yaw/upper-active">
			<default value="0"/>
			<test logic="OR" value="1">
				fcc/yaw/avail-upper-a eq 1
				fcc/yaw/avail-upper-b eq 1
			</test>
		</switch>
		
		<switch name="fcc/yaw/lower-active">
			<default value="0"/>
			<test logic="OR" value="1">
				fcc/yaw/avail-lower-a eq 1
				fcc/yaw/avail-lower-b eq 1
			</test>
		</switch>
		
		<summer name="fcc/yaw/channels-active">
			<input>fcc/yaw/upper-active</input>
			<input>fcc/yaw/lower-active</input>
		</summer>
	
	</channel>
	
	<channel name="RCWS Logic">
		
		<switch name="fcc/rcws/sys-1-active">
			<default value="0"/>
			<test logic="OR" value="0"> <!-- AP disc button kills RCWS -->
				/controls/cockpit/ap-disc-1 eq 1
				/controls/cockpit/ap-disc-2 eq 1
			</test>
			<test logic="AND" value="1">
				/it-autoflight/input/ovrd1 eq 0
				<test logic="OR">
					/systems/hydraulics/sys-2-psi ge 2200
					/systems/hydraulics/sys-3-psi ge 2200
				</test>
				fcc/fcc1-power eq 1
				/systems/iru-common/any-aligned-out eq 1
				fcc/yaw/channels-active gt 0
				/systems/acconfig/options/rcws-equipped eq 1
			</test>
		</switch>
		
		<switch name="fcc/rcws/sys-2-active">
			<default value="0"/>
			<test logic="OR" value="0"> <!-- AP disc button kills RCWS -->
				/controls/cockpit/ap-disc-1 eq 1
				/controls/cockpit/ap-disc-2 eq 1
			</test>
			<test logic="AND" value="1">
				/it-autoflight/input/ovrd2 eq 0
				<test logic="OR">
					/systems/hydraulics/sys-1-psi ge 2200
					/systems/hydraulics/sys-3-psi ge 2200
				</test>
				fcc/fcc2-power eq 1
				/systems/iru-common/any-aligned-out eq 1
				fcc/yaw/channels-active gt 0
				/systems/acconfig/options/rcws-equipped eq 1
			</test>
		</switch>
		
		<summer name="fcc/rcws/channels-active">
			<input>fcc/rcws/sys-1-active</input>
			<input>fcc/rcws/sys-2-active</input>
		</summer>
		
		<switch name="fcc/rcws/in-detent">
			<default value="fcc/rcws/in-detent"/>
			<test logic="AND" value="1">
				/controls/flight/aileron lt 0.02
				/controls/flight/aileron gt -0.02
				fcc/rcws/input lt 0.02
				fcc/rcws/input gt -0.02
			</test>
			<test logic="OR" value="0">
				/controls/flight/aileron gt 0.045
				/controls/flight/aileron lt -0.045
				fcc/rcws/input gt 0.045
				fcc/rcws/input lt -0.045
			</test>
		</switch>
		
		<switch name="fcc/rcws/active-no-wow">
			<default value="0"/>
			<test logic="AND" value="1">
				fcc/rcws/channels-active gt 0
				fcc/afs/enabled eq 0
			</test>
		</switch>
		
		<switch name="fcc/rcws/active">
			<default value="0"/>
			<test logic="AND" value="1">
				fcc/rcws/active-no-wow eq 1
				position/wow eq 0
			</test>
		</switch>
	
	</channel>
	
	<channel name="Gains">
		
		<fcs_function name="fcc/rcws/p-gain">
			<function>
				<product>
					<table>
						<independentVar lookup="row">velocities/vc-kts</independentVar>
						<tableData>
							140 -21
							360 -11
						</tableData>
					</table>
					<!--property>fcc/rcws/p-gain-tune</property--> <!-- For tuning -->
					<property>fcc/rcws/active</property>
				</product>
			</function>
		</fcs_function>
		
		<fcs_function name="fcc/rcws/i-gain">
			<function>
				<product>
					<table>
						<independentVar lookup="row">velocities/vc-kts</independentVar>
						<tableData>
							140 -28
							360 -14
						</tableData>
					</table>
					<!--property>fcc/rcws/i-gain-tune</property--> <!-- For tuning -->
					<property>fcc/rcws/active</property>
				</product>
			</function>
		</fcs_function>
		
		<fcs_function name="fcc/rcws/d-gain">
			<function>
				<product>
					<table>
						<independentVar lookup="row">velocities/vc-kts</independentVar>
						<tableData>
							140 -2.8
							360 -0.7
						</tableData>
					</table>
					<!--property>fcc/rcws/d-gain-tune</property--> <!-- For tuning -->
					<property>fcc/rcws/active</property>
				</product>
			</function>
		</fcs_function>
		
		<fcs_function name="fcc/lsas/pah/p-gain">
			<function>
				<product>
					<table>
						<independentVar lookup="row">velocities/vc-kts</independentVar>
						<tableData>
							140  25
							360  12
						</tableData>
					</table>
					<!--property>fcc/lsas/pah/p-gain-tune</property--> <!-- For tuning -->
					<property>fcc/lsas/pah/active</property>
				</product>
			</function>
		</fcs_function>
		
		<fcs_function name="fcc/lsas/pah/i-gain">
			<function>
				<product>
					<table>
						<independentVar lookup="row">velocities/vc-kts</independentVar>
						<tableData>
							140  15
							360   3
						</tableData>
					</table>
					<!--property>fcc/lsas/pah/i-gain-tune</property--> <!-- For tuning -->
					<property>fcc/lsas/pah/active</property>
				</product>
			</function>
		</fcs_function>
		
		<fcs_function name="fcc/lsas/pah/d-gain">
			<function>
				<product>
					<table>
						<independentVar lookup="row">velocities/vc-kts</independentVar>
						<tableData>
							140  5.0
							360  1.5
						</tableData>
					</table>
					<!--property>fcc/lsas/pah/d-gain-tune</property--> <!-- For tuning -->
					<property>fcc/lsas/pah/active</property>
				</product>
			</function>
		</fcs_function>
		
		<fcs_function name="fcc/lsas/prd/p-gain">
			<function>
				<product>
					<table>
						<independentVar lookup="row">velocities/vc-kts</independentVar>
						<tableData>
							140  9
							360  6
						</tableData>
					</table>
					<!--property>fcc/lsas/prd/p-gain-tune</property--> <!-- For tuning -->
					<table>
						<independentVar lookup="row">/instrumentation/altimeter/indicated-altitude-ft</independentVar>
						<tableData>
							16500  0.3
							20000  1.0
						</tableData>
					</table>
					<property>fcc/lsas/prd/active</property>
				</product>
			</function>
		</fcs_function>
		
		<fcs_function name="fcc/yaw/yd-p-gain">
			<function>
				<product>
					<table>
						<independentVar lookup="row">velocities/vc-kts</independentVar>
						<tableData>
							140 -7.5
							360 -1.0
						</tableData>
					</table>
					<!--property>fcc/yaw/yd-p-gain-tune</property--> <!-- For tuning -->
					<property>fcc/yaw/active</property>
				</product>
			</function>
		</fcs_function>
		
