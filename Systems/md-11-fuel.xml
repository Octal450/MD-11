			<input>-/systems/fuel/internal/aux-tank-upper-tank-1-inner</input>
			<input>-/systems/fuel/internal/aux-tank-upper-tank-2</input>
			<input>-/systems/fuel/internal/aux-tank-upper-tank-3-inner</input>
			<input>/systems/fuel/internal/aux-tank-lower-upper</input>
			<input>/systems/fuel/internal/tail-tank-aux-upper</input>
			<output>propulsion/tank[5]/external-flow-rate-pps</output>
		</summer>
		
		<summer name="/systems/fuel/internal/aux-tank-lower-flow-rate">
			<input>-/systems/fuel/internal/aux-tank-lower-upper</input>
			<output>propulsion/tank[6]/external-flow-rate-pps</output>
		</summer>
		
		<summer name="/systems/fuel/internal/tail-tank-flow-rate">
			<input>-/systems/fuel/internal/tail-tank-aux-upper</input>
			<input>-/systems/fuel/internal/tail-tank-eng-2</input>
			<output>propulsion/tank[7]/external-flow-rate-pps</output>
		</summer>
		
		<!-- Pipe Final Flow Rates -->
		<summer name="/systems/fuel/internal/eng-1-flow-rate">
			<input>/systems/fuel/internal/tank-1-inner-eng-1</input>
			<input>/systems/fuel/internal/tank-2-eng-1</input>
			<input>/systems/fuel/internal/tank-3-inner-eng-1</input>
			<output>propulsion/tank[8]/external-flow-rate-pps</output>
		</summer>
		
		<summer name="/systems/fuel/internal/eng-2-flow-rate">
			<input>/systems/fuel/internal/tank-1-inner-eng-2</input>
			<input>/systems/fuel/internal/tank-2-eng-2</input>
			<input>/systems/fuel/internal/tank-3-inner-eng-2</input>
			<input>/systems/fuel/internal/tail-tank-eng-2</input>
			<output>propulsion/tank[9]/external-flow-rate-pps</output>
		</summer>
		
		<summer name="/systems/fuel/internal/eng-3-flow-rate">
			<input>/systems/fuel/internal/tank-1-inner-eng-3</input>
			<input>/systems/fuel/internal/tank-2-eng-3</input>
			<input>/systems/fuel/internal/tank-3-inner-eng-3</input>
			<output>propulsion/tank[10]/external-flow-rate-pps</output>
		</summer>
	
	</channel>
	
	<channel name="Overhead Lights" execrate="8">
		
		<switch name="/controls/fuel/lights/select-manual">
			<default value="0"/>
			<test logic="AND" value="1">
				/controls/fuel/switches/system eq 1
				/systems/failures/fuel/system eq 1
			</test>
		</switch>
		
		<switch name="/controls/fuel/lights/fill-arm-1">
			<default value="1"/> <!-- ARM -->
			<test value="0">
				/controls/fuel/switches/fill-1 ne 1
			</test>
			<test logic="AND" value="1"> <!-- ARM -->
				/controls/fuel/switches/fill-1 eq 1
				propulsion/tank[1]/contents-lbs gt 33982
			</test>
			<test logic="OR" value="2"> <!-- FILL -->
				/systems/fuel/internal/aux-tank-upper-tank-1-inner-cmd eq 1
				/systems/fuel/internal/tank-2-tank-1-inner-cmd eq 1
				/systems/fuel/internal/tank-3-inner-tank-1-inner-cmd eq 1
			</test>
		</switch>
		
		<switch name="/controls/fuel/lights/fill-arm-2">
			<default value="1"/> <!-- ARM -->
			<test value="0">
				/controls/fuel/switches/fill-2 ne 1
			</test>
			<test logic="AND" value="1"> <!-- ARM -->
				/controls/fuel/switches/fill-2 eq 1
				propulsion/tank[2]/contents-lbs gt 64032
			</test>
			<test logic="OR" value="2"> <!-- FILL -->
				/systems/fuel/internal/aux-tank-upper-tank-2-cmd eq 1
				/systems/fuel/internal/tank-1-inner-tank-2-cmd eq 1
				/systems/fuel/internal/tank-3-inner-tank-2-cmd eq 1
			</test>
		</switch>
		
		<switch name="/controls/fuel/lights/fill-arm-3">
			<default value="1"/> <!-- ARM -->
			<test value="0">
				/controls/fuel/switches/fill-3 ne 1
			</test>
			<test logic="AND" value="1"> <!-- ARM -->
				/controls/fuel/switches/fill-3 eq 1
				propulsion/tank[3]/contents-lbs gt 33982
			</test>
			<test logic="OR" value="2"> <!-- FILL -->
				/systems/fuel/internal/aux-tank-upper-tank-3-inner-cmd eq 1
				/systems/fuel/internal/tank-1-inner-tank-3-inner-cmd eq 1
				/systems/fuel/internal/tank-2-tank-3-inner-cmd eq 1
			</test>
		</switch>
		
		<switch name="/controls/fuel/lights/pumps-trans-1-low">
			<default value="0"/>
			<test logic="OR" value="1">
				propulsion/tank[1]/contents-lbs le 100
				/systems/failures/fuel/pumps-1 eq 1
			</test>
		</switch>
		
		<switch name="/controls/fuel/lights/pumps-trans-2-low">
			<default value="0"/>
			<test logic="OR" value="1">
				propulsion/tank[2]/contents-lbs le 100
				/systems/failures/fuel/pumps-2 eq 1
			</test>
		</switch>
		
		<switch name="/controls/fuel/lights/pumps-trans-3-low">
			<default value="0"/>
			<test logic="OR" value="1">
				propulsion/tank[3]/contents-lbs le 100
				/systems/failures/fuel/pumps-3 eq 1
			</test>
		</switch>
		
		<switch name="/controls/fuel/lights/trans-aux-l-low">
			<default value="0"/>
			<test logic="OR" value="1">
				propulsion/tank[5]/contents-lbs le 100
				/systems/failures/fuel/pumps-aux-l eq 1
			</test>
		</switch>
		
		<switch name="/controls/fuel/lights/trans-aux-r-low">
			<default value="0"/>
			<test logic="OR" value="1">
				propulsion/tank[5]/contents-lbs le 100
				/systems/failures/fuel/pumps-aux-r eq 1
			</test>
		</switch>
		
		<switch name="/controls/fuel/lights/tail-low">
			<default value="0"/>
			<test logic="OR" value="1">
				propulsion/tank[7]/contents-lbs le 100
				/systems/failures/fuel/pumps-tail eq 1
			</test>
		</switch>
	
	</channel>
	
	<channel name="Fuel Indicators" execrate="16"> <!-- Rounds and sums appropriate tanks for good cockpit indication -->
		
		<fcs_function name="/systems/fuel/tank-1-lbs">
			<function>
				<product>
					<floor>
						<sum>
							<quotient>
								<sum>
									<property>propulsion/tank[0]/contents-lbs</property>
									<property>propulsion/tank[1]/contents-lbs</property>
								</sum>
								<value>50</value>
							</quotient>
							<value>0.5</value>
						</sum>
					</floor>
					<value>50</value>
				</product>
			</function>
		</fcs_function>
		
