			<default value="0"/>
			<test value="1">
				/systems/fuel/tail-aux eq 1
			</test>
		</switch>
	
	</channel>
	
	<!--
		0 - Tank 1 Outboard
		1 - Tank 1 Inboard
		2 - Tank 2
		3 - Tank 3 Inboard
		4 - Tank 3 Outboard
		5 - Upper Aux Tank
		6 - Lower Aux Tank
		7 - Tail Tank
		8 - Engine 1 Pipe
		9 - Engine 2 Pipe
		10 - Engine 3 Pipe
		11 - APU Pipe
