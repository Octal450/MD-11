		<switch name="/systems/fuel/trans-tail">
			<default value="0"/>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/r-emer-ac ge 112
				/controls/fuel/switches/trans-tail eq 1
				propulsion/tank[7]/contents-lbs gt 10
				/systems/fuel/system eq 1
				/systems/failures/fuel/pumps-tail ne 1
			</test>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/r-emer-ac ge 112
				/controls/fuel/switches/trans-tail eq 1
				/systems/fuel/system ne 1
				/systems/failures/fuel/pumps-tail ne 1
			</test>
		</switch>
		
		<pure_gain name="/systems/fuel/trans-tail-psi-cmd">
			<input>/systems/fuel/trans-tail</input>
			<gain>20</gain>
		</pure_gain>
		
		<actuator name="/systems/fuel/trans-tail-psi">
			<input>/systems/fuel/trans-tail-psi-cmd</input>
			<rate_limit sense="incr">5</rate_limit>
			<rate_limit sense="decr">10</rate_limit>
		</actuator>
		
		<switch name="/systems/fuel/tail-aux">
			<default value="0"/>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
