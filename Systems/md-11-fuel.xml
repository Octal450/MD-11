			</test>
		</switch>
		
		<switch name="/systems/fuel/internal/tank-2-eng-2">
			<default value="0"/>
			<test logic="AND" value="9">
				/systems/fuel/feed-2 eq 1
				propulsion/tank[2]/contents-lbs gt 10
