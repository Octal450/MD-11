<system name="MD-11: Fuel">
	
	<property value="10">/systems/fuel/internal/tank-2-balence-deadzone</property> <!-- Don't touch unless you know what you are doing! -->
	
	<channel name="FSC Switches/Logic" execrate="8">
		
		<switch name="/systems/fuel/system">
			<default value="/controls/fuel/switches/system"/>
			<test value="0">
				/systems/failures/fuel/system eq 1
			</test>
		</switch>
		
		<switch name="/systems/fuel/fsc/pumps-1">
			<default value="/controls/fuel/switches/pumps-1"/>
			<test logic="AND" value="0">
				/systems/fuel/system eq 1
				/engines/engine[0]/state eq 0
			</test>
			<test value="1">
				/systems/fuel/system eq 1
			</test>
			<output>/controls/fuel/switches/pumps-1</output>
		</switch>
		
		<switch name="/systems/fuel/fsc/pumps-2">
			<default value="/controls/fuel/switches/pumps-2"/>
			<test logic="AND" value="0">
				/systems/fuel/system eq 1
				/engines/engine[1]/state eq 0
				/engines/engine[3]/n2-actual lt 96
			</test>
			<test value="1">
				/systems/fuel/system eq 1
			</test>
			<output>/controls/fuel/switches/pumps-2</output>
		</switch>
		
		<switch name="/systems/fuel/fsc/pumps-3">
			<default value="/controls/fuel/switches/pumps-3"/>
			<test logic="AND" value="0">
				/systems/fuel/system eq 1
				/engines/engine[2]/state eq 0
			</test>
			<test value="1">
				/systems/fuel/system eq 1
			</test>
			<output>/controls/fuel/switches/pumps-3</output>
		</switch>
		
		<switch name="/systems/fuel/fsc/phase-6-timer-cmd">
			<default value="0"/>
			<test value="1">
				/fms/internal/phase eq 6
			</test>
		</switch>
		
		<actuator name="/systems/fuel/fsc/phase-6-timer">
			<input>/systems/fuel/fsc/phase-6-timer-cmd</input>
			<rate_limit sense="incr">0.01666666667</rate_limit> <!-- 1 minute -->
			<rate_limit sense="decr">1000</rate_limit> <!-- Instant -->
		</actuator>
		
		<switch name="/systems/fuel/fsc/fwd-pumps-cmd">
			<default value="0"/>
			<test logic="AND" value="1"> <!-- Turn off one minute after entering phase 6 -->
				/fms/internal/phase eq 6
				/systems/fuel/fsc/phase-6-timer lt 1
			</test>
			<test logic="OR" value="1">
				/fms/internal/phase eq 1
				/fms/internal/phase eq 5
			</test>
		</switch>
		
		<switch name="/systems/fuel/fsc/tank-1-fwd-pump">
			<default value="0"/>
			<test logic="AND" value="1">
				/controls/fuel/switches/pumps-1 eq 1
				/systems/fuel/fsc/fwd-pumps-cmd eq 1
			</test>
		</switch>
		
		<switch name="/systems/fuel/fsc/tank-2-fwd-pump">
			<default value="0"/>
			<test logic="AND" value="1">
				/controls/fuel/switches/pumps-2 eq 1
				/systems/fuel/fsc/fwd-pumps-cmd eq 1
			</test>
		</switch>
		
		<switch name="/systems/fuel/fsc/tank-3-fwd-pump">
			<default value="0"/>
			<test logic="AND" value="1">
				/controls/fuel/switches/pumps-3 eq 1
				/systems/fuel/fsc/fwd-pumps-cmd eq 1
			</test>
		</switch>
		
		<switch name="/systems/fuel/fsc/aux-pumps-cmd">
			<default value="1"/>
			<test logic="OR" value="0">
				/fms/internal/phase eq 1
				/fms/internal/phase eq 5
				/fms/internal/phase eq 6
			</test>
		</switch>
		
		<switch name="/systems/fuel/fsc/trans-aux-l">
			<default value="/controls/fuel/switches/trans-aux-l"/>
			<test logic="AND" value="1">
				/systems/fuel/system eq 1
				/systems/fuel/fsc/aux-pumps-cmd eq 1
				propulsion/tank[5]/contents-lbs gt 10
			</test>
			<test value="0">
				/systems/fuel/system eq 1
			</test>
			<output>/controls/fuel/switches/trans-aux-l</output>
		</switch>
		
		<switch name="/systems/fuel/fsc/trans-aux-r">
			<default value="/controls/fuel/switches/trans-aux-r"/>
			<test logic="AND" value="1">
				/systems/fuel/system eq 1
				/systems/fuel/fsc/aux-pumps-cmd eq 1
				propulsion/tank[5]/contents-lbs gt 10
			</test>
			<test value="0">
				/systems/fuel/system eq 1
			</test>
			<output>/controls/fuel/switches/trans-aux-r</output>
		</switch>
		
		<switch name="/systems/fuel/fsc/trans-tail">
			<default value="/controls/fuel/switches/trans-tail"/>
			<test logic="AND" value="1"> <!-- TODO: FSC tank management, for now use aux tank logic -->
				/systems/fuel/system eq 1
				/systems/fuel/fsc/aux-pumps-cmd eq 1
				propulsion/tank[7]/contents-lbs gt 10
			</test>
			<test value="0">
				/systems/fuel/system eq 1
			</test>
			<output>/controls/fuel/switches/trans-tail</output>
		</switch>
		
		<switch name="/systems/fuel/fsc/alt-pump">
			<default value="/controls/fuel/switches/alt-pump"/>
			<test value="0">
				/systems/fuel/system eq 1
			</test>
			<output>/controls/fuel/switches/alt-pump</output>
		</switch>
		
		<summer name="/systems/fuel/fsc/tank-1-level-bias">
			<input>propulsion/tank[0]/contents-lbs</input>
			<input>propulsion/tank[1]/contents-lbs</input>
			<input>/systems/fuel/internal/tank-2-balence-deadzone</input>
		</summer>
		
		<summer name="/systems/fuel/fsc/tank-3-level-bias">
			<input>propulsion/tank[3]/contents-lbs</input>
			<input>propulsion/tank[4]/contents-lbs</input>
			<input>/systems/fuel/internal/tank-2-balence-deadzone</input>
		</summer>
		
		<switch name="/systems/fuel/fsc/trans-1">
			<default value="/controls/fuel/switches/trans-1"/>
			<test value="0">
				/systems/fuel/system eq 1
			</test>
			<output>/controls/fuel/switches/trans-1</output>
		</switch>
		
		<switch name="/systems/fuel/fsc/trans-2">
			<default value="/controls/fuel/switches/trans-2"/>
			<test logic="AND" value="1">
				/systems/fuel/system eq 1
				propulsion/tank[2]/contents-lbs gt /systems/fuel/fsc/tank-1-level-bias
			</test>
			<test logic="AND" value="1">
				/systems/fuel/system eq 1
				propulsion/tank[2]/contents-lbs gt /systems/fuel/fsc/tank-3-level-bias
			</test>
			<test value="0">
				/systems/fuel/system eq 1
			</test>
			<output>/controls/fuel/switches/trans-2</output>
		</switch>
		
		<switch name="/systems/fuel/fsc/trans-3">
			<default value="/controls/fuel/switches/trans-3"/>
			<test value="0">
				/systems/fuel/system eq 1
			</test>
			<output>/controls/fuel/switches/trans-3</output>
		</switch>
		
		<switch name="/systems/fuel/fsc/fill-1">
			<default value="/controls/fuel/switches/fill-1"/>
			<test logic="AND" value="1">
				/systems/fuel/system eq 1
				/systems/fuel/fsc/aux-pumps-cmd eq 1
				propulsion/tank[5]/contents-lbs gt 10
			</test>
			<test logic="AND" value="1">
				/systems/fuel/system eq 1
				propulsion/tank[2]/contents-lbs gt /systems/fuel/fsc/tank-1-level-bias
			</test>
			<test value="0">
				/systems/fuel/system eq 1
			</test>
			<output>/controls/fuel/switches/fill-1</output>
		</switch>
		
		<switch name="/systems/fuel/fsc/fill-2">
			<default value="/controls/fuel/switches/fill-2"/>
			<test logic="AND" value="1">
				/systems/fuel/system eq 1
				/systems/fuel/fsc/aux-pumps-cmd eq 1
				propulsion/tank[5]/contents-lbs gt 10
			</test>
			<test value="0">
				/systems/fuel/system eq 1
			</test>
			<output>/controls/fuel/switches/fill-2</output>
		</switch>
		
		<switch name="/systems/fuel/fsc/fill-3">
			<default value="/controls/fuel/switches/fill-3"/>
			<test logic="AND" value="1">
				/systems/fuel/system eq 1
				/systems/fuel/fsc/aux-pumps-cmd eq 1
				propulsion/tank[5]/contents-lbs gt 10
			</test>
			<test logic="AND" value="1">
				/systems/fuel/system eq 1
				propulsion/tank[2]/contents-lbs gt /systems/fuel/fsc/tank-3-level-bias
			</test>
			<test value="0">
				/systems/fuel/system eq 1
			</test>
			<output>/controls/fuel/switches/fill-3</output>
		</switch>
		
		<switch name="/systems/fuel/fsc/x-feed-1">
			<default value="/controls/fuel/switches/x-feed-1"/>
			<test value="0">
				/systems/fuel/system eq 1
			</test>
			<output>/controls/fuel/switches/x-feed-1</output>
		</switch>
		
		<switch name="/systems/fuel/fsc/x-feed-2">
			<default value="/controls/fuel/switches/x-feed-2"/>
			<test value="0">
				/systems/fuel/system eq 1
			</test>
			<output>/controls/fuel/switches/x-feed-2</output>
		</switch>
		
		<switch name="/systems/fuel/fsc/x-feed-3">
			<default value="/controls/fuel/switches/x-feed-3"/>
			<test value="0">
				/systems/fuel/system eq 1
			</test>
			<output>/controls/fuel/switches/x-feed-3</output>
		</switch>
	
	</channel>
	
	<channel name="Main Fuel Logic" execrate="8">
		
		<!-- Tank Pumps -->
		<switch name="/systems/fuel/aft-pump-1">
			<default value="0"/>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-1 ge 112
				/controls/fuel/switches/pumps-1 eq 1
				/systems/failures/fuel/pumps-1 ne 1
			</test>
		</switch>
		
		<pure_gain name="/systems/fuel/aft-pump-1-psi-cmd">
			<input>/systems/fuel/aft-pump-1</input>
			<gain>20</gain>
		</pure_gain>
		
		<actuator name="/systems/fuel/aft-pump-1-psi">
			<input>/systems/fuel/aft-pump-1-psi-cmd</input>
			<rate_limit sense="incr">5</rate_limit>
			<rate_limit sense="decr">10</rate_limit>
		</actuator>
		
		<switch name="/systems/fuel/fwd-pump-1">
			<default value="0"/>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-2 ge 112
				/systems/fuel/system eq 1
				/systems/fuel/fsc/tank-1-fwd-pump eq 1
				/systems/failures/fuel/pumps-1 ne 1
			</test>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-2 ge 112
				/systems/fuel/system ne 1
				/controls/fuel/switches/pumps-1 eq 1
				/systems/failures/fuel/pumps-1 ne 1
			</test>
		</switch>
		
		<pure_gain name="/systems/fuel/fwd-pump-1-psi-cmd">
			<input>/systems/fuel/fwd-pump-1</input>
			<gain>20</gain>
		</pure_gain>
		
		<actuator name="/systems/fuel/fwd-pump-1-psi">
			<input>/systems/fuel/fwd-pump-1-psi-cmd</input>
			<rate_limit sense="incr">5</rate_limit>
			<rate_limit sense="decr">10</rate_limit>
		</actuator>
		
		<switch name="/systems/fuel/aft-pump-2l">
			<default value="0"/>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-2 ge 112
				/controls/fuel/switches/pumps-2 eq 1
				/systems/failures/fuel/pumps-2 ne 1
			</test>
			<test logic="AND" value="1">
				/systems/electrical/sources/adg/output-volt ge 112
				/controls/fuel/switches/pumps-2 eq 1
				/systems/failures/fuel/pumps-2 ne 1
			</test>
		</switch>
		
		<pure_gain name="/systems/fuel/aft-pump-2l-psi-cmd">
			<input>/systems/fuel/aft-pump-2l</input>
			<gain>20</gain>
		</pure_gain>
		
		<actuator name="/systems/fuel/aft-pump-2l-psi">
			<input>/systems/fuel/aft-pump-2l-psi-cmd</input>
			<rate_limit sense="incr">5</rate_limit>
			<rate_limit sense="decr">10</rate_limit>
		</actuator>
		
		<switch name="/systems/fuel/aft-pump-2r">
			<default value="0"/>
			<test logic="AND" value="0"> <!-- When in AUTO and engine 2 is off, only left aft pump runs -->
				/systems/fuel/system eq 1
				/engines/engine[1]/state eq 0
			</test>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-1 ge 112
				/controls/fuel/switches/pumps-2 eq 1
				/systems/failures/fuel/pumps-2 ne 1
			</test>
		</switch>
		
		<pure_gain name="/systems/fuel/aft-pump-2r-psi-cmd">
			<input>/systems/fuel/aft-pump-2r</input>
			<gain>20</gain>
		</pure_gain>
		
		<actuator name="/systems/fuel/aft-pump-2r-psi">
			<input>/systems/fuel/aft-pump-2r-psi-cmd</input>
			<rate_limit sense="incr">5</rate_limit>
			<rate_limit sense="decr">10</rate_limit>
		</actuator>
		
		<switch name="/systems/fuel/fwd-pump-2">
			<default value="0"/>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-3 ge 112
				/systems/fuel/system eq 1
				/systems/fuel/fsc/tank-2-fwd-pump eq 1
				/systems/failures/fuel/pumps-2 ne 1
			</test>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-3 ge 112
				/systems/fuel/system ne 1
				/controls/fuel/switches/pumps-2 eq 1
				/systems/failures/fuel/pumps-2 ne 1
			</test>
		</switch>
		
		<pure_gain name="/systems/fuel/fwd-pump-2-psi-cmd">
			<input>/systems/fuel/fwd-pump-2</input>
			<gain>20</gain>
		</pure_gain>
		
		<actuator name="/systems/fuel/fwd-pump-2-psi">
			<input>/systems/fuel/fwd-pump-2-psi-cmd</input>
			<rate_limit sense="incr">5</rate_limit>
			<rate_limit sense="decr">10</rate_limit>
		</actuator>
		
		<switch name="/systems/fuel/apu-start-pump">
			<default value="0"/>
			<test logic="AND" value="1">
				/systems/electrical/bus/dc-bat ge 24
				/systems/apu/dc-fuel-pump eq 1
			</test>
		</switch>
		
		<pure_gain name="/systems/fuel/apu-start-pump-psi-cmd">
			<input>/systems/fuel/apu-start-pump</input>
			<gain>20</gain>
		</pure_gain>
		
		<actuator name="/systems/fuel/apu-start-pump-psi">
			<input>/systems/fuel/apu-start-pump-psi-cmd</input>
			<rate_limit sense="incr">5</rate_limit>
			<rate_limit sense="decr">10</rate_limit>
		</actuator>
		
		<switch name="/systems/fuel/aft-pump-3">
			<default value="0"/>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-3 ge 112
				/controls/fuel/switches/pumps-3 eq 1
				/systems/failures/fuel/pumps-3 ne 1
			</test>
		</switch>
		
		<pure_gain name="/systems/fuel/aft-pump-3-psi-cmd">
			<input>/systems/fuel/aft-pump-3</input>
			<gain>20</gain>
		</pure_gain>
		
		<actuator name="/systems/fuel/aft-pump-3-psi">
			<input>/systems/fuel/aft-pump-3-psi-cmd</input>
			<rate_limit sense="incr">5</rate_limit>
			<rate_limit sense="decr">10</rate_limit>
		</actuator>
		
		<switch name="/systems/fuel/fwd-pump-3">
			<default value="0"/>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-2 ge 112
				/systems/fuel/system eq 1
				/systems/fuel/fsc/tank-3-fwd-pump eq 1
				/systems/failures/fuel/pumps-3 ne 1
			</test>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-2 ge 112
				/systems/fuel/system ne 1
				/controls/fuel/switches/pumps-3 eq 1
				/systems/failures/fuel/pumps-3 ne 1
			</test>
		</switch>
		
		<pure_gain name="/systems/fuel/fwd-pump-3-psi-cmd">
			<input>/systems/fuel/fwd-pump-3</input>
			<gain>20</gain>
		</pure_gain>
		
		<actuator name="/systems/fuel/fwd-pump-3-psi">
			<input>/systems/fuel/fwd-pump-3-psi-cmd</input>
			<rate_limit sense="incr">5</rate_limit>
			<rate_limit sense="decr">10</rate_limit>
		</actuator>
		
		<!-- Crossfeed Valves -->
		<actuator name="/systems/fuel/x-feed-1-valve">
			<input>/controls/fuel/switches/x-feed-1</input>
			<rate_limit>0.333</rate_limit> <!-- 3 seconds -->
		</actuator>
		
		<actuator name="/systems/fuel/x-feed-2-valve">
			<input>/controls/fuel/switches/x-feed-2</input>
			<rate_limit>0.333</rate_limit> <!-- 3 seconds -->
		</actuator>
		
		<actuator name="/systems/fuel/x-feed-3-valve">
			<input>/controls/fuel/switches/x-feed-3</input>
			<rate_limit>0.333</rate_limit> <!-- 3 seconds -->
		</actuator>
		
		<!-- Transfer Pumps -->
		<switch name="/systems/fuel/trans-pump-1">
			<default value="0"/>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-1 ge 112
				/controls/fuel/switches/trans-1 eq 1
				/systems/failures/fuel/pumps-1 ne 1
			</test>
		</switch>
		
		<pure_gain name="/systems/fuel/trans-pump-1-psi-cmd">
			<input>/systems/fuel/trans-pump-1</input>
			<gain>20</gain>
		</pure_gain>
		
		<actuator name="/systems/fuel/trans-pump-1-psi">
			<input>/systems/fuel/trans-pump-1-psi-cmd</input>
			<rate_limit sense="incr">5</rate_limit>
			<rate_limit sense="decr">10</rate_limit>
		</actuator>
		
		<switch name="/systems/fuel/trans-pump-2">
			<default value="0"/>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-2 ge 112
				/controls/fuel/switches/trans-2 eq 1
				/systems/failures/fuel/pumps-2 ne 1
			</test>
		</switch>
		
		<pure_gain name="/systems/fuel/trans-pump-2-psi-cmd">
			<input>/systems/fuel/trans-pump-2</input>
			<gain>20</gain>
		</pure_gain>
		
		<actuator name="/systems/fuel/trans-pump-2-psi">
			<input>/systems/fuel/trans-pump-2-psi-cmd</input>
			<rate_limit sense="incr">5</rate_limit>
			<rate_limit sense="decr">10</rate_limit>
		</actuator>
		
		<switch name="/systems/fuel/trans-pump-3">
			<default value="0"/>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-3 ge 112
				/controls/fuel/switches/trans-3 eq 1
				/systems/failures/fuel/pumps-3 ne 1
			</test>
		</switch>
		
		<pure_gain name="/systems/fuel/trans-pump-3-psi-cmd">
			<input>/systems/fuel/trans-pump-3</input>
			<gain>20</gain>
		</pure_gain>
		
		<actuator name="/systems/fuel/trans-pump-3-psi">
			<input>/systems/fuel/trans-pump-3-psi-cmd</input>
			<rate_limit sense="incr">5</rate_limit>
			<rate_limit sense="decr">10</rate_limit>
		</actuator>
		
		<switch name="/systems/fuel/trans-aux-upper-l">
			<default value="0"/>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-1 ge 112
				/controls/fuel/switches/trans-aux-l eq 1
				/systems/failures/fuel/pumps-aux-l ne 1
			</test>
		</switch>
		
		<pure_gain name="/systems/fuel/trans-aux-upper-l-psi-cmd">
			<input>/systems/fuel/trans-aux-upper-l</input>
			<gain>20</gain>
		</pure_gain>
		
		<actuator name="/systems/fuel/trans-aux-upper-l-psi">
			<input>/systems/fuel/trans-aux-upper-l-psi-cmd</input>
			<rate_limit sense="incr">5</rate_limit>
			<rate_limit sense="decr">10</rate_limit>
		</actuator>
		
		<switch name="/systems/fuel/trans-aux-upper-r">
			<default value="0"/>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-2 ge 112
				/controls/fuel/switches/trans-aux-r eq 1
				/systems/failures/fuel/pumps-aux-r ne 1
			</test>
		</switch>
		
		<pure_gain name="/systems/fuel/trans-aux-upper-r-psi-cmd">
			<input>/systems/fuel/trans-aux-upper-r</input>
			<gain>20</gain>
		</pure_gain>
		
		<actuator name="/systems/fuel/trans-aux-upper-r-psi">
			<input>/systems/fuel/trans-aux-upper-r-psi-cmd</input>
			<rate_limit sense="incr">5</rate_limit>
			<rate_limit sense="decr">10</rate_limit>
		</actuator>
		
		<switch name="/systems/fuel/trans-aux-lower-l">
			<default value="0"/>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-2 ge 112
				/controls/fuel/switches/trans-aux-l eq 1
				propulsion/tank[6]/contents-lbs gt 10
				/systems/fuel/system eq 1
				/systems/failures/fuel/pumps-aux-l ne 1
			</test>
			<test logic="AND" value="1"> <!-- Power source is guess, unknown -->
				/systems/electrical/bus/ac-2 ge 112
				/controls/fuel/switches/trans-aux-l eq 1
				/systems/fuel/system ne 1
				/systems/failures/fuel/pumps-aux-l ne 1
