		<mdapu>
			<name>apu</name>
			<mode>looped</mode>
			<path>Aircraft/MD-11/Sounds/FL2070/11_apu.wav</path>
			<property>engines/engine[3]/n2-actual</property>
			<condition>
				<and>
					<greater-than>
						<property>engines/engine[3]/n2-actual</property>
						<value>1</value>
					</greater-than>
					<property>sim/current-view/internal</property>
				</and>
			</condition>
			
			<volume>
				<property>engines/engine[3]/n2-actual</property>
				<factor>0.0005</factor>
			</volume>
			<pitch>
				<property>engines/engine[3]/n2-actual</property>
				<factor>0.008</factor>
			</pitch>
		</mdapu>
		
