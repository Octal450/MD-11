			<volume>
				<factor>0.75</factor>
			</volume>
		</switch>
		
		<switch>
			<name>Knob</name>
			<mode>once</mode>
			<path>Aircraft/MD-11/Sounds/Cockpit/knob.wav</path>
			<condition>
				<property>/sim/sound/knb1</property>
			</condition>
			<volume>
				<factor>0.60</factor>
			</volume>
		</switch>
		
		<switch>
			<name>Switch</name>
			<mode>once</mode>
			<path>Aircraft/MD-11/Sounds/Cockpit/switch.wav</path>
			<condition>
				<property>/sim/sound/switch1</property>
			</condition>
			<volume>
				<factor>0.75</factor>
			</volume>
		</switch>
		
		<cvrtone>
			<name>CVRtone</name>
			<mode>once</mode>
			<path>Aircraft/MD-11/Sounds/Cockpit/700hz.wav</path>
			<condition>
				<equals>
					<property>/controls/CVR/tone</property>
					<value>1</value>
				</equals>
			</condition>
			<volume>
				<factor>0.60</factor>
			</volume>
		</cvrtone>
		
