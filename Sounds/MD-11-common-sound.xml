<PropertyList>
	<fx>
		
		<apoff>
			<name>apoff</name>
			<mode>looped</mode>
			<path>Aircraft/MD-11/Sounds/apoff.wav</path>
			<condition>
				<and>
					<property>/it-autoflight/sound/apoffsound</property>
					<or>
						<greater-than-equals>
							<property>/systems/electrical/bus/dc-1</property>
							<value>25</value>
						</greater-than-equals>
						<greater-than-equals>
							<property>/systems/electrical/bus/dc-2</property>
							<value>25</value>
						</greater-than-equals>
						<greater-than-equals>
							<property>/systems/electrical/bus/dc-3</property>
							<value>25</value>
						</greater-than-equals>
					</or>
				</and>
			</condition>
			<volume>
				<factor>10.0</factor>
			</volume>
		</apoff>
		
		<sb>
			<mode>once</mode>
			<name>sb</name>
			<path>Aircraft/MD-11/Sounds/Cockpit/seatbelt.wav</path>
			<condition>
				<and>
					<greater-than-equals>
						<property>/systems/electrical/outputs/efis</property>
						<value>25</value>
					</greater-than-equals>
					<property>/sim/sound/seatbelt-sign</property>
				</and>
			</condition>
			<volume>
				<property>/sim/current-view/internal</property>
			</volume>
		</sb>
		
		<ns>
			<mode>once</mode>
			<name>ns</name>
			<path>Aircraft/MD-11/Sounds/Cockpit/nosmoko.wav</path>
			<condition>
				<and>
					<greater-than-equals>
						<property>/systems/electrical/outputs/efis</property>
						<value>25</value>
					</greater-than-equals>
					<property>/sim/sound/no-smoking-sign</property>
				</and>
			</condition>
			<volume>
				<property>/sim/current-view/internal</property>
			</volume>
		</ns>
		
		<switch>
			<name>Clamp on</name>
			<mode>once</mode>
			<path>Aircraft/MD-11/Sounds/Cockpit/clamp.wav</path>
			<condition>
				<property>/it-autoflight/output/clamp</property>
			</condition>
			<volume>
				<factor>0.95</factor>
			</volume>
		</switch>
		
		<switch>
			<name>Clamp off</name>
			<mode>once</mode>
			<path>Aircraft/MD-11/Sounds/Cockpit/clamp.wav</path>
			<condition>
				<not><property>/it-autoflight/output/clamp</property></not>
			</condition>
			<volume>
				<factor>0.95</factor>
			</volume>
		</switch>
		
		<switch>
			<name>Button1</name>
			<mode>once</mode>
			<path>Aircraft/MD-11/Sounds/Cockpit/button.wav</path>
			<condition>
				<property>/sim/sound/btn1</property>
			</condition>
			<volume>
				<factor>0.75</factor>
			</volume>
		</switch>
		
		<switch>
			<name>ButtonOH</name>
			<mode>once</mode>
			<path>Aircraft/MD-11/Sounds/Cockpit/button2.wav</path>
			<condition>
				<property>/sim/sound/oh-btn</property>
			</condition>
			<volume>
				<factor>0.75</factor>
			</volume>
		</switch>
		
		<switch>
			<name>Button3</name>
			<mode>once</mode>
			<path>Aircraft/MD-11/Sounds/Cockpit/button3.wav</path>
			<condition>
