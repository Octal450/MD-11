	<layout>vbox</layout>
    <pref-width>260</pref-width>

    <group>
        <layout>hbox</layout>
        <text>
            <halign>left</halign>
            <label>IT-AUTOFLIGHT System (MD-11)</label>
        </text>
        <button>
            <halign>right</halign>
            <pref-width>20</pref-width>
            <pref-height>20</pref-height>
            <legend>X</legend>
            <key>Esc</key>
            <binding>
                <command>dialog-close</command>
            </binding>
        </button>
    </group>
	
