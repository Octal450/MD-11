<PropertyList>

 <path>green-navlight.ac</path>
 <nopreview/>



 <effect>
   <inherits-from>Aircraft/MD-11/Models/lights/ALS/green-navlight</inherits-from>
   <object-name>green-navlightals</object-name>
 </effect>
 
 <animation>
  <type>scale</type>
  <x-offset>0.55</x-offset>
  <y-offset>0.55</y-offset>
  <z-offset>0.55</z-offset>
 </animation>
 
<animation>
  <type>dist-scale</type>
  <object-name>green-navlightals</object-name>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>1</dep>
   </entry>
   <entry>
    <ind>300</ind>
    <dep>4</dep>
   </entry>
   <entry>
    <ind>1500</ind>
    <dep>8</dep>
   </entry>
  </interpolation>
 </animation>

</PropertyList>
