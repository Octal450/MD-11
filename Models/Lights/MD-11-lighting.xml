		<type>select</type>
		<object-name>Beacon.Lower</object-name>
		<object-name>Beacon.Upper</object-name>
		<condition>
			<property>sim/model/lights/beacon/state</property>
		</condition>
    </animation>
	
	<!-- Lights change -->
	
	<animation>
		<type>select</type>
		<object-name>GreenLight</object-name>
		<object-name>RedLight</object-name>
		<object-name>Strobelight.1</object-name>
		<object-name>Strobelight.2</object-name>
		<object-name>Strobelight.3</object-name>
		<object-name>Strobelight.4</object-name>
		<object-name>Rearnav.1</object-name>
		<object-name>Rearnav.2</object-name>
		<object-name>Beacon.Lower</object-name>
		<object-name>Beacon.Upper</object-name>
		<condition>
			<and>
				<not><property>sim/rendering/shaders/skydome</property></not>
				<or>
					<greater-than-equals>
						<property>systems/electrical/bus/l-emer-ac</property>
						<value>112</value>
					</greater-than-equals>
					<greater-than-equals>
						<property>systems/electrical/bus/r-emer-ac</property>
						<value>112</value>
					</greater-than-equals>
				</or>
			</and>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>GreenLightALS</object-name>
		<object-name>RedLightALS</object-name>
		<object-name>Strobelight.1ALS</object-name>
		<object-name>Strobelight.2ALS</object-name>
		<object-name>Strobelight.3ALS</object-name>
		<object-name>Strobelight.4ALS</object-name>
		<object-name>Rearnav.1ALS</object-name>
		<object-name>Rearnav.2ALS</object-name>
		<object-name>Beacon.LowerALS</object-name>
		<object-name>Beacon.UpperALS</object-name>
		<object-name>LandLALS</object-name>
		<object-name>LandRALS</object-name>
		<condition>
			<and>
				<property>sim/rendering/shaders/skydome</property>
				<or>
					<greater-than-equals>
						<property>systems/electrical/bus/l-emer-ac</property>
						<value>112</value>
					</greater-than-equals>
					<greater-than-equals>
						<property>systems/electrical/bus/r-emer-ac</property>
						<value>112</value>
					</greater-than-equals>
				</or>
			</and>
		</condition>
	</animation>

