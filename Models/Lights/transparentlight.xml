<PropertyList>

	<path>transparentlight.ac</path>

	<animation>
		<type>flash</type>
		<object-name>Light</object-name>
		<power>10</power>
		<factor>3</factor>
		<min>1</min>
		<max>6</max>
		<two-sides type="bool">false</two-sides>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
