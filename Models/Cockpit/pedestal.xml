	<animation>
		<name>Integ Panel lighting</name>
		<type>material</type>
		<object-name>pedestal.text</object-name>
		<object-name>aileron.light</object-name>
		<object-name>rudder.light</object-name>
		<object-name>mcdu_glare</object-name>
		<object-name>DialAFlapLabel</object-name>
		<object-name>ADGHandleLabel</object-name>
		<emission>
			<red>1.0</red>
			<green>0.85</green>
			<blue>0.6</blue>
			<factor-prop>systems/lighting/main-integ</factor-prop>
		</emission>
	</animation>
	
	<!-- No button lights without power! -->
	<animation>
		<type>select</type>
		<object-name>SlatStow.on</object-name>
		<condition>
			<or>
				<greater-than-equals>
					<property>systems/electrical/bus/dc-1</property>
					<value>24</value>
				</greater-than-equals>
				<greater-than-equals>
					<property>systems/electrical/bus/dc-2</property>
					<value>24</value>
				</greater-than-equals>
				<greater-than-equals>
					<property>systems/electrical/bus/dc-3</property>
					<value>24</value>
				</greater-than-equals>
			</or>
		</condition>
	</animation>
	
