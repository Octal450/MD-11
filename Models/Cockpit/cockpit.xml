	<!-- Light Shades -->	
	<animation>
		<type>knob</type>
		<object-name>lightshade.left.handle</object-name>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>controls/cockpit/shade-left-cmd</property>
				<factor>0.05</factor>
				<min>0</min>
				<max>1</max>
				<wrap>false</wrap>
			</binding>
		</action>
		<release>
			<binding>
				<command>nasal</command>
				<script>
					if (pts.Controls.Cockpit.shadeLeftCmd.getValue() >= 0.5) {
						pts.Controls.Cockpit.shadeLeftCmd.setValue(1);
					} else {
						pts.Controls.Cockpit.shadeLeftCmd.setValue(0);
					}
				</script>
			</binding>
		</release>
	</animation>
	
