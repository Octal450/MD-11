<PropertyList>
 <path>screenselector.ac</path>
 
         <animation>
		<name>Panel lighting</name>
		<type>material</type>
		<object-name>screenselector.text</object-name>
		<object-name>config.text</object-name>
		<object-name>status.text</object-name>
		<object-name>misc.text</object-name>
        <object-name>conseq.text</object-name>
		<object-name>nd.text</object-name>
		<emission>
			<red>1.5</red>
			<green>1.27</green>
			<blue>1</blue>
			<factor-prop>controls/lighting/pedestal-panel-norm</factor-prop>
		</emission>
	</animation>
	
				    <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
		<object-name>screenselector.text</object-name>
    </effect>
    
    					    	<animation>
		<name>first screen brighteness</name>
		<type>pick</type>
		<object-name>scrn.1.knob</object-name>
		<action>
			<button>0</button>
			<button>3</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/lighting/screen-1-norm</property>
				<step>0.025</step>
				<max>1</max>
			</binding>
		</action>
		<action>
			<button>1</button>
			<button>4</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/lighting/screen-1-norm</property>
				<step>-0.025</step>
				<min>0.050</min>
			</binding>
		</action>
	</animation>
	
