		<menubar>
			<default>
				<menu n="100">
					<label>IDG-MD-11X</label>
					<enabled type="bool">true</enabled>
					<item>
						<label>Aircraft Config</label>
						<binding>
							<command>nasal</command>
							<script>
							acconfig.main_dlg.open();
							</script>
						</binding>
					</item>
					<item>
						<label>About This Aircraft</label>
						<binding>
							<command>nasal</command>
							<script>
							acconfig.about_dlg.open();
							</script>
						</binding>
					</item>
				</menu>
				<menu n="101">
					<label>Utilities</label>
					<enabled type="bool">true</enabled>
					<item>
						<label>Select Livery</label>
						<binding>
							<command>nasal</command>
							<script>
							aircraft.livery.dialog.toggle();
							</script>
						</binding>
					</item>
					<item>
						<label>Pushback</label>
						<binding>
							<command>dialog-show</command>
							<dialog-name>pushback</dialog-name>
						</binding>
					</item>
					<item>
						<label>Skip IRS</label>
						<binding>
							<command>property-assign</command>
							<property>/controls/irs/skip</property>
							<value>1</value>
						</binding>
					</item>
					<item>
						<label>Show Captain's PFD</label>
						<binding>
							<command>nasal</command>
							<script>
							canvas_pfd.showPFD1();
							</script>
						</binding>
					</item>
					<item>
						<label>Show First Officer's PFD</label>
						<binding>
							<command>nasal</command>
							<script>
							canvas_pfd.showPFD2();
							</script>
						</binding>
					</item>
					<item>
						<label>Show EAD</label>
						<binding>
							<command>nasal</command>
							<script>
							canvas_ead.showEAD();
							</script>
						</binding>
					</item>
				</menu>
			</default>
		</menubar>
        
